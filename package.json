{
  "name": "grunner",
  "version": "0.0.32",
  "description": "GRunner is a task runner for Node.js compatible with Gulp plugins",
  "main": "index.js",
  "watch": {
    "test": [
      "index.js",
      "{bin,test}/*.js"
    ]
  },
  "scripts": {
    "test": "node ./test/index.js",
    "push": "node -p \"new Date().toISOString();\" && npm publish ./",
    "watch": "npm-watch"
  },
  "author": "madebits <contact@madebits.33mail.com> (https://github.com/madebits/)",
  "license": "MIT",
  "dependencies": {
    "async": "^1.5.2",
    "mkdirp": "^0.5.1",
    "require-dir": "^0.3.0",
    "through2": "^2.0.0",
    "yargs": "^3.32.0"
  },
  "devDependencies": {
    "gspawn": "0.0.3",
    "gulp": "^3.9.0",
    "npm-watch": "^0.1.1",
    "tape": "^4.4.0"
  },
  "bin": "./bin/grunner.js",
  "engines": {
    "node": ">= 5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/madebits/nodejs-grunner.git"
  },
  "keywords": [
    "gulp",
    "task",
    "automation",
    "build",
    "make",
    "tool",
    "pipeline",
    "stream"
  ]
}
